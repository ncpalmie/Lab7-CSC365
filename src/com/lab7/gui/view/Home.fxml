<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="https://javafx.com/fxml" fx:controller="com.lab7.gui.controller.HomeController">
    <StackPane>
        <VBox>
            <Button fx:id="navHome" text="Home" onAction="#pressButton"/>
            <Button fx:id="navBook" text="Book a Reservation" onAction="#pressButton"/>
            <Button fx:id="navAlter" text="Modify a Reservation" onAction="#pressButton"/>
            <Button fx:id="navCurrent" text="View a Reservation" onAction="#pressButton"/>
            <Button fx:id="navRevenue" text="See Revenue Details" onAction="#pressButton"/>
        </VBox>
    </StackPane>
    <StackPane>
        <Label fx:id="label" text="Home"/>
    </StackPane>
</VBox>