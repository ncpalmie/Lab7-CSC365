<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="https://javafx.com/fxml" fx:controller="com.lab7.gui.controller.SceneController">
    <StackPane>
        <HBox>
            <Button fx:id="nav1" text="Go To Scene 1!" onAction="#pressButton"/>
            <Button fx:id="nav2" text="Go To Scene 2!" onAction="#pressButton"/>
        </HBox>
    </StackPane>
    <StackPane>
        <Label fx:id="label" text="Scene 2"/>
    </StackPane>
</VBox>